<Window x:Class="RumahSakit.View.DasboardAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RumahSakit.View"
        mc:Ignorable="d"
        Title="DasboardAdmin" Height="588" Width="1123.333" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <TabControl>
        <TabItem Header="Dokter">
            <Grid Background="White">
                <DataGrid Name="dgDokter" HorizontalAlignment="Left" Height="529" VerticalAlignment="Top" Width="774" Margin="-2,0,0,0">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="ID"/>
                        <DataGridTextColumn Header="Nama"/>
                        <DataGridTextColumn Header="Email"/>
                        <DataGridTextColumn Header="Jenis Kelamin"/>
                        <DataGridTextColumn Header="Alamat"/>
                        <DataGridTextColumn Header="No Telp"/>
                    </DataGrid.Columns>
                </DataGrid>
                <Grid Background="#00C853" HorizontalAlignment="Left" Height="529" Margin="774,0,0,0" VerticalAlignment="Top" Width="335">
                </Grid>

            </Grid>
        </TabItem>
        <TabItem Header="Perawatan">
            <Grid Background="White">
                <DataGrid Name="dgPerawat" AutoGenerateColumns="False" HorizontalAlignment="Left" Height="529" VerticalAlignment="Top" Width="774" Margin="-2,0,0,0" SelectionChanged="dgPerawat_SelectionChanged">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="ID" Width="50" Binding="{Binding NURSE_ID}"/>
                        <DataGridTextColumn Header="Nama" Width="180" Binding="{Binding NAME}"/>
                        <DataGridTextColumn Header="Email" Width="150" Binding="{Binding EMAIL}"/>
                        <DataGridTextColumn Header="Alamat" Width="200" Binding="{Binding ADDRESS}"/>
                        <DataGridTextColumn Header="No Telp" Width="100" Binding="{Binding PHONE}"/>
                        <DataGridTextColumn Header="Jenis Kelamin" Width="90" Binding="{Binding GENDER}"/>
                    </DataGrid.Columns>
                </DataGrid>
                <Grid Background="#00C853" HorizontalAlignment="Left" Height="529" Margin="774,0,0,0" VerticalAlignment="Top" Width="335">
                    <Label Content="Nama" HorizontalAlignment="Left" Margin="73,130,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Email" HorizontalAlignment="Left" Margin="77,168,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Alamat" HorizontalAlignment="Left" Margin="63,205,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="No Telp" HorizontalAlignment="Left" Margin="67,276,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Jenis Kelamin" HorizontalAlignment="Left" Margin="27,309,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <TextBox Name="txtNamaPer" HorizontalAlignment="Left" Height="25" Margin="131,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <TextBox Name="txtEmailPer" HorizontalAlignment="Left" Height="25" Margin="131,170,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <TextBox Name="txtTelpPer" HorizontalAlignment="Left" Height="25" Margin="131,280,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" BorderThickness="0"/>
                    <TextBox Name="txtAlamatPer" HorizontalAlignment="Left" Height="60" Margin="131,207,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" BorderThickness="0"/>
                    <RadioButton Name="rbLakiN" Content="Laki-laki" HorizontalAlignment="Left" Margin="131,319,0,0" VerticalAlignment="Top" BorderThickness="0" Foreground="White"/>
                    <RadioButton Name="rbPerempuanN" Content="Perempuan " HorizontalAlignment="Left" Margin="208,320,0,0" VerticalAlignment="Top" BorderThickness="0" Foreground="White"/>
                    <Button Name="btnTambahN" Content="Tambah" HorizontalAlignment="Left" Margin="38,357,0,0" VerticalAlignment="Top" Width="75" Height="29" Foreground="#FF00C853" Background="White" BorderThickness="0" Click="btnTambahN_Click_1"/>
                    <Button Name="btnEditN" Content="Edit" HorizontalAlignment="Left" Margin="124,357,0,0" VerticalAlignment="Top" Width="75" Height="29" Foreground="#FF00C853" Background="White" BorderThickness="0" Click="btnEditN_Click"/>
                    <Button Name="btnDeleteN" Content="Delete" HorizontalAlignment="Left" Margin="212,357,0,0" VerticalAlignment="Top" Width="75" Height="29" Foreground="#FF00C853" Background="White" BorderThickness="0" Click="btnDeleteN_Click"/>
                </Grid>

            </Grid>
        </TabItem>
        <TabItem Header="Pasien">
            <Grid Background="White">
                <DataGrid AutoGenerateColumns="False" Name="dgPasien" HorizontalAlignment="Left" Height="529" VerticalAlignment="Top" Width="774" Margin="-2,0,0,0" SelectionChanged="dgPasien_SelectionChanged">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="ID" Width="50" Binding="{Binding PATIENT_ID}"/>
                        <DataGridTextColumn Header="Nama" Width="180" Binding="{Binding NAME}"/>
                        <DataGridTextColumn Header="Tanggal Lahir" Width="100" Binding="{Binding BIRTH}"/>
                        <DataGridTextColumn Header="Alamat" Width="200" Binding="{Binding ADDRESS}"/>
                        <DataGridTextColumn Header="No Telp" Width="100" Binding="{Binding PHONE}"/>
                        <DataGridTextColumn Header="Umur" Width="50" Binding="{Binding AGE}"/>
                        <DataGridTextColumn Header="Jenis Kelamin" Width="90" Binding="{Binding GENDER}"/>
                    </DataGrid.Columns>
                </DataGrid>

                <Grid Background="#00C853" HorizontalAlignment="Left" Height="529" Margin="774,0,0,0" VerticalAlignment="Top" Width="335">
                    <Label Content="Nama" HorizontalAlignment="Left" Margin="71,131,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Tanggal Lahir" HorizontalAlignment="Left" Margin="26,167,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.36,1.974" FontSize="14" Foreground="White"/>
                    <Label Content="Alamat" HorizontalAlignment="Left" Margin="65,201,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="No Telp" HorizontalAlignment="Left" Margin="65,240,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Umur" HorizontalAlignment="Left" Margin="74,278,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Jenis Kelamin" HorizontalAlignment="Left" Margin="29,309,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <TextBox Name="txtNamaP" HorizontalAlignment="Left" Height="25" Margin="129,136,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <TextBox Name="txtAlamatP" HorizontalAlignment="Left" Height="25" Margin="129,205,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <TextBox Name="txtTelpP" HorizontalAlignment="Left" Height="25" Margin="129,243,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <TextBox Name="txtUmurP" HorizontalAlignment="Left" Height="25" Margin="129,281,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <DatePicker Name="dtTanggalLahirP" HorizontalAlignment="Left" Margin="129,170,0,0" VerticalAlignment="Top" Width="155" BorderThickness="0"/>
                    <RadioButton Name="rbLaki" Content="Laki-laki" HorizontalAlignment="Left" Margin="129,317,0,0" VerticalAlignment="Top" BorderThickness="0" Foreground="White"/>
                    <RadioButton Name="rbPerempuan" Content="Perempuan" HorizontalAlignment="Left" Margin="205,317,0,0" VerticalAlignment="Top" BorderThickness="0" Foreground="White"/>
                    <Button Name="btnTambah" Content="Tambah" HorizontalAlignment="Left" Margin="37,361,0,0" VerticalAlignment="Top" Width="75" Height="29" Foreground="#FF00C853" Background="White" BorderThickness="0" Click="btnTambah_Click"/>
                    <Button Name="btnEdit" Content="Edit" HorizontalAlignment="Left" Margin="123,361,0,0" VerticalAlignment="Top" Width="75" Height="29" Background="White" Foreground="#FF00C853" BorderThickness="0" Click="btnEdit_Click"/>
                    <Button Name="btnDelete" Content="Delete" HorizontalAlignment="Left" Margin="209,361,0,0" VerticalAlignment="Top" Width="75" Height="29" Foreground="#FF00C853" Background="White" BorderThickness="0" Click="btnDelete_Click"/>
                    <Label Name="tempJK" Content="" HorizontalAlignment="Left" Margin="284,498,0,0" VerticalAlignment="Top"/>

                </Grid>

            </Grid>
        </TabItem>
        <TabItem Header="Obat">
            <Grid Background="White">
                <DataGrid Name="dgObat" HorizontalAlignment="Left" Height="529" VerticalAlignment="Top" Width="774" Margin="-2,0,0,0"/>
                <Grid Background="#00C853" HorizontalAlignment="Left" Height="529" Margin="774,0,0,0" VerticalAlignment="Top" Width="335">
                    <Label Content="Nama" HorizontalAlignment="Left" Margin="71,115,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Tipe Obat" HorizontalAlignment="Left" Margin="50,155,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.36,1.974" FontSize="14" Foreground="White"/>
                    <Label Content="Expired" HorizontalAlignment="Left" Margin="65,230,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Stock" HorizontalAlignment="Left" Margin="75,266,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
                    <Label Content="Harga" HorizontalAlignment="Left" Margin="71,192,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.36,1.974" FontSize="14" Foreground="White"/>
                    <TextBox HorizontalAlignment="Left" Height="23" Margin="152,115,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                    <TextBox HorizontalAlignment="Left" Height="23" Margin="152,192,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                    <TextBox HorizontalAlignment="Left" Height="23" Margin="152,281,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                    <ComboBox HorizontalAlignment="Left" Margin="152,155,0,0" VerticalAlignment="Top" Width="120"/>
                    <DatePicker HorizontalAlignment="Left" Margin="152,234,0,0" VerticalAlignment="Top" Width="120"/>
                    <Button Content="Button" HorizontalAlignment="Left" Margin="50,327,0,0" VerticalAlignment="Top" Width="75"/>
                    <Button Content="Button" HorizontalAlignment="Left" Margin="142,327,0,0" VerticalAlignment="Top" Width="75"/>
                    <Button Content="Button" HorizontalAlignment="Left" Margin="231,327,0,0" VerticalAlignment="Top" Width="75"/>

                </Grid>


            </Grid>
        </TabItem>
    </TabControl>
</Window>
    
    
